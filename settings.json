{
  "base_keymap": "VSCode",
  "telemetry": {
    "diagnostics": true,
    "metrics": true
  },
  "buffer_font_family": "Jetbrains Mono NL",
  "buffer_font_size": 12,
  "projects_online_by_default": false,
  "soft_wrap": "editor_width",
  "language_overrides": {
    "Python": {
      // "format_on_save": {
      //     "external": {
      //         "command": "black",
      //         "arguments": [
      //             "-"
      //         ]
      //     }
      // }
    },
    "C": {
      "tab_size": 4,
      "format_on_save": "off"
    },
    "C++": {
      "tab_size": 4,
      "format_on_save": "off"
    },
    "JavaScript": {
      "tab_size": 4
    },
    "JSON": {
      "tab_size": 2
    },
    "TypeScript": {
      "tab_size": 4
    },
    "TSX": {
      "tab_size": 4
    }
  },
  "theme": "staff/Atelier Heath Light",
  "journal": {
    "hour_format": "hour12"
  },
  // The factor to grow the active pane by. Defaults to 1.0
  // which gives the same size as all other panes.
  // "active_pane_magnification": 2.0,
  "staff_mode": true
}
