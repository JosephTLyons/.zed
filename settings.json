{
  "vim_mode": false,
  "chat_panel": {
    "dock": "right"
  },
  "assistant": {
    "dock": "left"
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrains Mono NL",
  "buffer_font_size": 14,
  "calls": {
    "mute_on_join": true
  },
  "collaboration_panel": {
    "dock": "left"
  },
  "copilot": {
    "disabled_globs": [".env", "debug_1.txt"]
  },
  "elixir": {
    "lsp": "next_ls"
  },
  "extend_comment_on_newline": false,
  "inlay_hints": {
    "enabled": false,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true
  },
  "journal": {
    "hour_format": "hour12"
  },
  "language_overrides": {
    "Uiua": {
      "show_copilot_suggestions": false
    },
    "Plain Text": {
      "show_copilot_suggestions": false
    },
    "Python": {
      "format_on_save": "off",
      "formatter": {
        "external": {
          "command": "black",
          "arguments": ["-"]
        }
      }
    },
    "C": {
      "tab_size": 4,
      "format_on_save": "off"
    },
    "C++": {
      "tab_size": 4,
      "format_on_save": "off"
    },
    "JavaScript": {
      "tab_size": 4
    },
    "JSON": {
      "tab_size": 2
    },
    "TypeScript": {
      "tab_size": 4,
      "formatter": "prettier"
    },
    "TSX": {
      "tab_size": 4
    },
    "YAML": {
      "tab_size": 4
    }
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "useParameterNames": true,
            "enable": "skip_trivial"
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        }
      },
      "typescript-language-server": {
        "initialization_options": {
          "preferences": {
            "includeInlayParameterNameHints": "all",
            "includeInlayParameterNameHintsWhenArgumentMatchesName": true,
            "includeInlayFunctionParameterTypeHints": true,
            "includeInlayVariableTypeHints": true,
            "includeInlayVariableTypeHintsWhenTypeMatchesName": false,
            "includeInlayPropertyDeclarationTypeHints": true,
            "includeInlayFunctionLikeReturnTypeHints": true,
            "includeInlayEnumMemberValueHints": true
          }
        }
      }
    }
  },
  "project_panel": {
    "dock": "right"
  },
  "notification_panel": {
    "dock": "left"
  },
  "projects_online_by_default": false,
  "scrollbar": {
    "git_diff": true,
    "selections": true,
    "show": "auto"
  },
  "semantic_index": {
    "enabled": true
  },
  "staff_mode": true,
  "seed_search_query_from_cursor": "selection",
  "show_whitespaces": "selection",
  "soft_wrap": "editor_width",
  "tabs": {
    "git_status": true
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true
  },
  "theme": "Atelier Estuary Dark",
  "terminal": {
    "dock": "bottom"
  },
  "ui_font_family": "Jetbrains Mono NL",
  "wrap_guides": [80]
}
