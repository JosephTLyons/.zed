[
  {
    "context": "Editor",
    "bindings": {
      "cmd-d": "editor::DuplicateLine",
      "cmd-shift-d": "editor::SelectNext",
      "shift-alt-up": "editor::AddSelectionAbove",
      "shift-alt-down": "editor::AddSelectionBelow",
      "ctrl-alt-shift-down": "editor::GoToHunk",
      "ctrl-alt-shift-up": "editor::GoToPrevHunk",
      "cmd-/": [
        "editor::ToggleComments",
        {
          "advance_downwards": true
        }
      ]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-\\": "workspace::ToggleLeftSidebar",
      "cmd-|": "collab::ToggleContactsMenu"
    }
  },
  {
    "bindings": {
      "cmd-alt-,": "zed::OpenKeymap"
    }
  }
]
